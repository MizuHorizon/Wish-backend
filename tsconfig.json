{
    "compilerOptions": {
      "target": "ES2016",
     
      "module": "commonjs",
      "resolveJsonModule": true,
      "outDir": "dist",
      "strict": true,
      "noImplicitAny": false,
      "esModuleInterop": true,
      "skipLibCheck": false,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src/**/*"]
  }